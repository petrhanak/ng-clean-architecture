{
  "name": "desktop",
  "version": "0.6.0",
  "main": "entrypoint/out/main.js",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run build:entry && ng build --base-href ./ -c production && electron-builder build",
    "build:entry": "tsc -p entrypoint/tsconfig.serve.json --outDir entrypoint/out",
    "ng:serve": "ng serve",
    "electron:serve": "wait-on tcp:4200 && npm run build:entry && electron . --serve"
  },
  "dependencies": {
    "@angular/common": "~11.2.1",
    "@angular/compiler": "~11.2.1",
    "@angular/core": "~11.2.1",
    "@angular/platform-browser": "~11.2.1",
    "@angular/platform-browser-dynamic": "~11.2.1",
    "@angular/router": "~11.2.1",
    "rxjs": "~6.6.6",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "~11.1.0",
    "@angular-devkit/build-angular": "~0.1102.3",
    "@angular/compiler-cli": "~11.2.1",
    "electron": "12.0.0",
    "electron-builder": "^22.10.5",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.1.5",
    "wait-on": "^5.2.1"
  }
}
